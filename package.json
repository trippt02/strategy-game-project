{
  "name": "strategy-game-project",
  "version": "1.0.0",
  "description": "An infinite strategy game where players explore, collect resources, and build their empire.",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:watch": "webpack --watch",
    "build:dev:watch": "webpack --mode development --watch",
    "build:prod:watch": "webpack --mode production --watch",
    "build:dev:analyze": "webpack --mode development --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "build:prod:analyze": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "build:dev:analyze:watch": "webpack --mode development --profile --json > stats.json && webpack-bundle-analyzer stats.json --watch",
    "build:prod:analyze:watch": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json --watch",
    "build:dev:analyze:report": "webpack --mode development --profile --json > stats.json && webpack-bundle-analyzer stats.json --report",
    "build:prod:analyze:report": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json --report",
    "build:dev:analyze:report:watch": "webpack --mode development --profile --json > stats.json && webpack-bundle-analyzer stats.json --report --watch",
    "build:prod:analyze:report:watch": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json --report --watch",
    "build:dev:analyze:report:json": "webpack --mode development --profile --json > stats.json && webpack-bundle-analyzer stats.json --report --json",
    "build:prod:analyze:report:json": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json --report --json",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "keywords": [
    "strategy",
    "game",
    "infinite",
    "resources",
    "building"
  ],
  "author": "Timothy Tripp",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-env": "^7.26.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.99.5",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "devDependencies": {
    "babel-loader": "^8.2.3",
    "babel-preset-react": "^6.24.1",
    "jest": "^27.0.6"
  }
}
